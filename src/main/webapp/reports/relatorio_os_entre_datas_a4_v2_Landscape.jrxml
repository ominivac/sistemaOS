	<queryString language="SQL">
		<![CDATA[SELECT os.cod_os,
	os.data_previsao,
	item.valor_parcial,
	item.quantidade_horas,
	produto_os.descricao,
	os.valor_total
FROM item
	INNER JOIN os ON 
	 item.os_cod = os.cod_os 
	INNER JOIN produto_os ON 
	 item.produto_cod = produto_os.cod_produto 
